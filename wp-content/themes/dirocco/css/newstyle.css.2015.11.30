/*.SLIDER*/

#box {
    text-align: left;
    width: 100%;
    margin: 0;
    padding: 0;
    overflow: hidden;
    -webkit-animation-name: fadeIn;
    -webkit-animation-duration: 3s;
    -webkit-animation-iteration-count: 1;
    -webkit-animation-delay: 0s;
}
ul#slider {
    margin: 0;
    padding: 0;
    list-style: none;
    position: relative;
    width: 100%;
    height: 530px;
    overflow: hidden;
}
ul#thumb {
    overflow: none;
    margin: 0;
    padding: 0;
    list-style: none;
    position: relative;
    overflow: auto;
    width: 98.66666%;
    margin-right: 1.3333333%;
}
ul#thumb a {
    -webkit-transition: opacity .2s ease-in-out;
    width: 32%;
    height: 100%;
    display: block;
    overflow: hidden;
    float: right;
    margin: 20px 0 0 1.3333%;
    opacity: 0.8;
}
ul#thumb a img {
    width: 100%;
    height: 100%;
}
ul#thumb a:hover {
    opacity: 1;
}
ul#slider li {
    width: 100%;
    height: 100%;
    position: relative;
}
ul#slider li img {
    height: 100%;
    width: 100%;
}
ul#slider li p {
    font-family: 'WhitneyLight','WhitneyBook' !important;

    font-size: 18px !important;
    position: absolute;
    bottom: 45%;
    width: 25%;
    margin-left: 7%;
    text-align: center;
    left: 0;
    z-index: inherit;
    color: #fff;
}



ul#slider li h2 {

    font-family: 'WhitneyBold','WhitneyBook' !important;

    font-size: 46px !important;

    position: absolute;

    bottom: 57%;

    width: 25%;

    margin-left: 7%;

    text-align: center;

    left: 0;

    z-index: inherit;

    color: #fff;

}






ul#slider li h3 {

    font-family: 'WhitneyLight','WhitneyBook' !important;

    font-size: 22px !important;

    position: absolute;

    bottom: 70%;

    width: 25%;

    margin-left: 7%;

    text-align: center;

    left: 0;

    z-index: inherit;

    color: #fff;

}


ul#slider li p span {
    line-height: 1.2em;
    padding: 10px;
    display: block;
}
/* Animation for the :target image. Slides the image in. */

@-webkit-keyframes moveTarget {
    0% {
        left: -100%;
    }
    100% {
        left: 0px;
    }
}
ul#slider li:target {
    -webkit-animation-name: moveTarget;
    -webkit-animation-duration: .5s;
    -webkit-animation-iteration-count: 1;
    top: 0px;
    left: 0px;
    z-index: 10;
}
ul#slider li:not(:target) {
    -webkit-animation-name: moveIt;
    -webkit-animation-duration: 1.5s;
    -webkit-animation-iteration-count: 1;
    top: 0px;
    left: 0px;
}
