/* HEADER hamburger menu */
.whopper-container {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex; 
    -webkit-align-items: center; 
        -ms-flex-align: center; 
            align-items: center;
    position: absolute;
    top: 34px;
    left: 45px;
    transition: all .25s;
}

.whopper {
    margin: 13px 0 17px;
}

.whopper span {
    padding-left: 20px;
    font-family: Helvetica;
    font-weight: bold;
    font-size: 22px;
    letter-spacing: 2px;
}

.close-text {
    display: none;
}

.whopper .menu {
    height: 20px;
    width: 20px;
}

.whopper .bar {
    background-color: black;
    height: 4px;
    width: 20px;
    transition: all .25s;
    position: relative;
}

.whopper .one {
    top: 5px;
}

.whopper .two {
    top: 10px;
}

.whopper:hover .one,
.whopper:hover .two {
    background-color : gray;
}
.whopper .one-ani {
    -webkit-transform: rotate(33deg);
            transform: rotate(33deg);
    top: 7px;
    transition: all .25s;
}
.whopper .two-ani {
  -webkit-transform: rotate(-33deg);
          transform: rotate(-33deg);
  top: 3px;
  transition: all .25s;
}
/* EOF HEADER hamburger menu */

.navbar-menu {
    width: 400px;
    height: 50px;
    transition: all .25s;
    overflow: hidden;
}

.navbar-menu-hidden {
    width: 0;
    height: 0;
}

.navbar-menu-mobile {
    width: 100%;
